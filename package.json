{
  "name": "which-cdn",
  "version": "1.0.0",
  "private": true,
  "description": "A tool to detect CDN providers for websites",
  "main": "server.js",
  "type": "module",
  "engines": {
    "node": ">=22.0.0"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "node server.js",
    "test": "npm run lint && npm run format",
    "format": "prettier --single-quote --check .",
    "format:fix": "prettier --single-quote --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": [
    "cdn",
    "detection",
    "dns",
    "web"
  ],
  "author": "Mark J. Becker",
  "license": "Apache-2.0",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "eslint": "^9.33.0",
    "prettier": "^3.6.2"
  }
}
